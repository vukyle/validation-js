    "css-loader": "^0.28.11",
    "del-cli": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.0",
